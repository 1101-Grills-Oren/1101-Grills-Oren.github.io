{
  "compilerOptions": {
    "target": "es6",
    "lib": [
      "dom",
      "es2019"
    ],
    "module": "esnext",
    "strict": true,
    "moduleResolution": "node",
    "esModuleInterop": true,
    "downlevelIteration": true,
    "forceConsistentCasingInFileNames": true,
    "resolveJsonModule": true,
    "baseUrl": ".",
    "paths": {
      "@mcschema/*": [
        "node_modules/@mcschema/*",
        "*"
      ]
    }
  },
  "include": [
    "src", "app_.ts"
  ]
}
