{
  "compilerOptions": {
    "target": "es6",
    "lib": [
      "dom",
      "es2017.object",
      "es2019"
    ],
    "module": "es6",
    "strict": true,
    "moduleResolution": "node",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "resolveJsonModule": true,
    "baseUrl": "."
  },
  "include": [
    "src",
  ]
}
